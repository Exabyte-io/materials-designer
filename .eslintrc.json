{
    "extends": ["@exabyte-io/eslint-config", "plugin:react/recommended"],
    "plugins": ["react"],
    "rules": {
        "react/jsx-filename-extension": "error",
        "jsx-a11y/label-has-associated-control": ["error", { "assert": "either" }],
        "react/forbid-prop-types": "warn"
    }
}

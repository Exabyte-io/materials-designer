{
    "extends": ["@exabyte-io/eslint-config"],
    "settings": {
        "import/resolver": {
            "node": {
                "extensions": [".ts"]
            }
        }
    },
    "plugins": ["react"],
    "rules": {
        "import/no-extraneous-dependencies": "off",
        "class-methods-use-this": "off"
    }
}

{
    "name": "@exabyte-io/materials-designer",
    "version": "2022.1.9-0",
    "description": "Materials Designer",
    "scripts": {
        "start": "cross-env PORT=3001 SKIP_PREFLIGHT_CHECK=true react-scripts start",
        "prepublishOnly": "rm -rf dist; npm run transpile; npm run transpile-tests",
        "transpile-tests": "babel --presets @babel/preset-env --out-dir dist/tests/cucumber tests/cucumber",
        "transpile": "babel --out-dir dist src; node-sass -r src/stylesheets -o dist/stylesheets",
        "test": "bash run-tests.sh",
        "lint": "eslint src && prettier --write src",
        "lint:fix": "eslint src --fix && prettier --write src",
        "postinstall": "npm run transpile",
        "prettier": "prettier --check src",
        "prepare": "husky install"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Exabyte-io/materials-designer.git"
    },
    "main": "dist/exports.js",
    "author": "Exabyte Inc.",
    "bugs": {
        "url": "https://github.com/Exabyte-io/materials-designer/issues"
    },
    "license": "Apache-2.0",
    "homepage": "https://github.com/Exabyte-io/materials-designer",
    "dependencies": {
        "@babel/cli": "7.16.0",
        "@babel/core": "7.16.0",
        "@babel/plugin-proposal-class-properties": "7.16.0",
        "@babel/preset-env": "7.16.4",
        "@babel/preset-react": "7.16.7",
        "@babel/runtime": "7.16.7",
        "@babel/runtime-corejs2": "7.16.7",
        "@exabyte-io/wave.js": "2022.10.13-0",
        "@material-ui/core": "^4.12.3",
        "@material-ui/icons": "^4.11.2",
        "@uiw/react-codemirror": "^3.2.7",
        "bootstrap": "^3.3.7",
        "classnames": "2.3.1",
        "codemirror": "^5.47.0",
        "jquery": "3.6.0",
        "lodash": "^4.17.4",
        "mathjs": "^3.9.0",
        "mixwith": "^0.1.1",
        "node-sass": "^4.11.0",
        "prop-types": "^15.8.0",
        "react-bootstrap": "0.32.4",
        "react-full-screen": "^0.2.2",
        "react-jss": "^8.6.1",
        "react-redux": "^4.4.8",
        "react-s-alert": "^1.4.1",
        "react-scripts": "^4.0.3",
        "redux": "^3.6.0",
        "redux-batched-subscribe": "^0.1.6",
        "redux-logger": "^3.0.6",
        "redux-undo": "^1.0.0-beta9-9-7",
        "sprintf-js": "^1.1.2",
        "underscore": "^1.12.1",
        "underscore.string": "^3.3.4"
    },
    "peerDependencies": {
        "@exabyte-io/made.js": "*",
        "react": "^16.14.0",
        "react-dom": "^16.14.0"
    },
    "devDependencies": {
        "@exabyte-io/made.js": "2022.5.5-3",
        "babel-preset-react-app": "^9.1.2",
        "cross-env": "^7.0.3",
        "eslint": "7.32.0",
        "eslint-config-airbnb": "19.0.2",
        "eslint-config-prettier": "8.3.0",
        "eslint-plugin-import": "2.25.3",
        "eslint-plugin-jsdoc": "37.1.0",
        "eslint-plugin-jsx-a11y": "6.5.1",
        "eslint-plugin-prettier": "4.0.0",
        "eslint-plugin-react": "7.27.1",
        "eslint-plugin-simple-import-sort": "7.0.0",
        "husky": "7.0.4",
        "lint-staged": "12.1.2",
        "prettier": "2.5.1",
        "react": "^16.14.0",
        "react-dom": "^16.14.0",
        "react-error-overlay": "^6.0.9"
    },
    "lint-staged": {
        "*.{js,jsx}": "eslint --cache --fix",
        "*.{js,jsx,css}": "prettier --write"
    },
    "resolutions": {
        "react-error-overlay": "6.0.9"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "engines": {
        "node": ">=12.0",
        "npm": ">=6.14"
    }
}
